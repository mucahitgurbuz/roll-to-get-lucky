{
  "defaultSeverity": "error",
  "extends": ["tslint-config-airbnb", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "variable-name": false,
    "function-name": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "object-shorthand-properties-first": false,
    "import-name": false,
    "max-line-length": [true, 120]
  },
  "rulesDirectory": []
}
